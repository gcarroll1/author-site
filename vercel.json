{
  "functions": {
    "app/**/route.{js,ts}": { "regions": ["syd1"] },
    "api/**/*.{js,ts}": { "regions": ["syd1"] }
  },
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "greatbookierobbery.com.au" }],
      "destination": "https://gregorymcarroll.com/great-bookie-robbery"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "greatbookierobbery.au" }],
      "destination": "https://gregorymcarroll.com/great-bookie-robbery"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "greatbookierobbery.com" }],
      "destination": "https://gregorymcarroll.com/great-bookie-robbery"
    }
  ]
}
